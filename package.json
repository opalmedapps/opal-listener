{
  "name": "opal-listener",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "docs": "run-script-os",
    "docs:nix": "./node_modules/.bin/jsdoc src README.md -c docs/jsdoc.config.json",
    "docs:windows": ".\\node_modules\\.bin\\jsdoc src README.md -c docs/jsdoc.config.json",
    "test": "cross-env DISABLED_LOGGING=true mocha './src/**/*.test.*' './listener/**/*.test.*' './legacy-registration/**/*.test.*'",
    "coverage": "nyc mocha './src/**/*.test.*' './listener/**/*.test.*' './legacy-registration/**/*.test.*'",
    "start": "node src/server.js",
    "watch": "nodemon src/server.js",
    "watch:docker": "nodemon -L src/server.js",
    "simulateRequest": "node src/test/simulate-request",
    "simulateRequestDocker": "docker exec opal-listener node src/test/simulate-request"
  },
  "dependencies": {
    "@stablelib/base64": "1.0.1",
    "@stablelib/utf8": "1.0.2",
    "axios": "1.7.9",
    "cross-env": "7.0.3",
    "crypto-js": "4.2.0",
    "dotenv": "16.4.7",
    "express-validator": "7.2.1",
    "firebase-admin": "13.0.2",
    "html-to-text": "9.0.5",
    "keyv": "5.2.3",
    "moment": "2.30.1",
    "mysql": "2.18.1",
    "node-cron": "3.0.3",
    "nodemailer": "6.9.16",
    "nodemon": "3.1.9",
    "q": "1.5.1",
    "tweetnacl": "1.0.3",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "chai": "4.5.0",
    "chai-as-promised": "7.1.2",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.1",
    "eslint-plugin-mocha": "10.5.0",
    "firebase-tools": "13.29.1",
    "jsdoc": "4.0.4",
    "mocha": "11.0.1",
    "nyc": "17.1.0",
    "run-script-os": "1.1.6",
    "sinon": "19.0.2"
  },
  "keywords": [
    "muhc",
    "opal",
    "health",
    "oncology"
  ],
  "author": "David Herrera",
  "license": "ISC",
  "homepage": "https://depdocs.com/opal",
  "description": "Backend listener for the Opal patient portal"
}
