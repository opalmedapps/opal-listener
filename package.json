{
  "name": "opal-listener",
  "version": "1.0.0",
  "scripts": {
    "lint": "eslint src/**/*.{js,ts,jsx,tsx}",
    "docs": "run-script-os",
    "docs:nix": "./node_modules/.bin/jsdoc src README.md -c docs/jsdoc.config.json",
    "docs:windows": ".\\node_modules\\.bin\\jsdoc src README.md -c docs/jsdoc.config.json",
    "test": "mocha './src/**/*.test.*'",
    "start": "node src/server.js",
    "watch": "nodemon src/server.js",
    "watch:docker": "nodemon -L src/server.js"
  },
  "dependencies": {
    "@stablelib/base64": "1.0.1",
    "@stablelib/utf8": "1.0.1",
    "crypto-js": "4.1.1",
    "express-validator": "6.14.0",
    "firebase-admin": "10.0.2",
    "moment": "2.29.1",
    "mysql": "2.18.1",
    "nodemailer": "6.7.2",
    "nodemon": "2.0.15",
    "q": "1.5.1",
    "request": "2.88.2",
    "tweetnacl": "1.0.3",
    "winston": "3.6.0"
  },
  "devDependencies": {
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "chai-datetime": "1.8.0",
    "eslint": "8.8.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-mocha": "10.0.3",
    "jsdoc": "3.6.10",
    "mocha": "9.2.1",
    "run-script-os": "1.1.6",
    "tui-jsdoc-template": "1.2.2"
  },
  "keywords": [
    "muhc",
    "opal",
    "health",
    "oncology"
  ],
  "author": "David Herrera",
  "license": "ISC",
  "homepage": "https://depdocs.com/opal",
  "description": "Backend listener for the Opal patient portal"
}
