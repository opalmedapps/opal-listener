{
  "opts": {
    "encoding": "utf8",
    "destination": "docs/generated",
    "recurse": true,
    "tutorials": "docs/source"
  },
  "plugins": ["plugins/markdown"],
  "recurseDepth": 20,
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "((^|\\/|\\\\)_)|(.+\\.test\\..+)"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  }
}
