{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2024,
    "sourceType": "module"
  },
  "extends": [
    "plugin:import/recommended"
  ],
  "rules": {
    "import/extensions": ["error", "ignorePackages"],
    "sort-imports": ["error", {
      "ignoreCase": true,
      "ignoreDeclarationSort": true
    }]
  }
}
